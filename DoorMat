N = (input().split(" "))
n, m = int(N[0]), int(N[1])
c,d = 1, 1
for i in range(n):
   
    if (i<(n//2)):
        temp = (".|."*c)
        print (temp.center(m,"-"))
        c+=2
    elif(i==n//2):
        print("WELCOME".center(m, "-"))

    elif (i>(n//2)):
        c-=2
        temp = (".|."*c)
        print (temp.center(m,"-"))
        

