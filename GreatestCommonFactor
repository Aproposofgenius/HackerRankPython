def Division(num1,num2): 
    x=[]
    y=[]
    for i in range(1, num1+1):
        if num1%i==0:
            x.append(i)
            
    for i in range(1, num2+1):
        if num2%i==0:
            y.append(i)
            
    x = set(x)
    y = set(y)
    z = max(x.intersection(y))
    
    return z
    
# keep this function call here  
print Division(raw_input())
